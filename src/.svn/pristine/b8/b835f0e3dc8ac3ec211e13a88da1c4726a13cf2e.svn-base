.leftContainer{
  width: auto;
  float: left;
  position: relative;
  overflow: auto;
}
.rightContainer{
  width: -webkit-calc(100% - 81px);
  width: calc(100% - 81px);
  float: right;
  position: relative;
  overflow: auto;
  /*background: #f9f9f9;*/
}

.navContainer{
  width: 180px;
}

.ant-menu-inline, .ant-menu-vertical, .ant-menu-vertical-left{
  border-right: none;
}
.ant-menu{
  background: none;
}
/*ant导航宽度修改*/
.ant-menu-inline-collapsed > .ant-menu-item, .ant-menu-inline-collapsed > .ant-menu-item-group > .ant-menu-item-group-list > .ant-menu-item, .ant-menu-inline-collapsed > .ant-menu-submenu > .ant-menu-submenu-title{
  padding:0 18px !important;
}
.ant-menu-submenu.ant-menu-submenu-popup.ant-menu-light.ant-menu-submenu-placement-rightTop{
  left: 60px !important;
}
.ant-menu-inline-collapsed{
  width: 56px;
}
