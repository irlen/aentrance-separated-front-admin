
import React, { Component } from 'react'
import { withRouter } from 'react-router-dom'
class MyRedirect extends Component{
  componentDidMount(){
    this.props.history.push('/login')
  }
  render(){
    return(
      <div>
        您沒有登陸
      </div>
    )
  }
}
export default withRouter(MyRedirect)
